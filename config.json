{
    "tasksPath": "./gulp/tasks",
    "dockerContainersPath": "./containers",
    "dockerProject":"boldapproach",
    "dockerMachine":"default",
    "scssSrcPath": "./src/scss",
    "lessSrcPath": "./src/less",
    "jsSrcPath":["./node_modules/angular/angular.js","./src/**/*.js"],
    "jsDistPath":"./app/js",
    "jadeSrcPath": "./src",
    "imgSrcPath": "./src/img",
    "cssDistPath": "./app/public",
    "lessDistPath": "./app/public",
    "jadeDistPath": "./app",
    "imgDistPath": "./app/img",
    "browserList": "> 1%', 'last 2 versions', 'Firefox ESR', 'Opera 12.1",
    "browserSync": {
        "open": false,
        "proxy": "http://example.dev",
        "socket": {
            "domain": "http://192.168.1.127:3000"
        },
        "browser": ["google-chrome"],
        "notify": true
    },
    "browserSyncWatch":["./app/**/*.*"]
}
